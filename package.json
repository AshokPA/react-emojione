    "emoji",
    "emojione",
    "emojify",
    "unicode",
    "smileys"
  ],
  "bugs": {
    "url": "https://github.com/pladaria/react-emojione/issues"
  },
  "homepage": "https://github.com/pladaria/react-emojione#readme",
